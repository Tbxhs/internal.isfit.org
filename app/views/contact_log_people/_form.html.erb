<%= bootstrap_form_for(@contact_log_person) do |f| %>
  <%= f.error_messages %>
  <%= f.fieldset "contact_log_person" do %>
          <%= f.text_field :first_name , class: "span6"%>
          <%= f.text_field :last_name , class: "span6"%>
          <%= f.text_field :email , class: "span6"%>
          <%= f.text_field :phone , class: "span6"%>
      <% end %>
  <%= f.submit "Submit", class: "btn" %>
<% end %>
