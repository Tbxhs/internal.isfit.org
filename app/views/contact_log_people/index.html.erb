<h1>Kontaktlogg</h1>

<div class="contact-log-person">
  <%= link_to "Create contact-log-person", new_contact_log_person_path, :class => "primary btn" %>
  <% for contact_log_person in @contact_log_people %>
    <div class="contact-log-unit">
      <div class="action-menu">
        <%= link_to :edit, edit_contact_log_person_path(contact_log_person), :class => "small btn"%>
        <%= link_to :delete, contact_log_person, confirm: "Are you sure?", method: :delete, :class => "small btn"  %>
      </div>
        <p class="small">Created <%= time_ago_in_words(contact_log_person.created_at) %> ago by <%#= link_to contact_log.user.full_name, contact_log.user %>.</p>
       <p>
      <%= contact_log_person.first_name %>
      </p>
    </div>
  <% end %>
</div>
