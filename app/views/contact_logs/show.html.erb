<h1>Contactlog</h1>
<div class="contact-log">
<table>
  <tr><td>Contact unit:</td><td><%= link_to @contact_log.contact_log_person.contact_log_unit.company_name, @contact_log.contact_log_person.contact_log_unit %></td></tr>
  <tr><td>Contact person:</td><td><%= link_to @contact_log.contact_log_person.full_name, @contact_log.contact_log_person %></td></tr>
  <tr><td>Created by:</td><td><%= link_to @contact_log.user.full_name, @contact_log.user %></td></tr>
  <tr><td>Time: </td><td><%= time_ago_in_words(@contact_log.created_at) %> ago </td></tr>
  <tr><td>Content:</td><td><%= @contact_log.content %> </td></tr>
</table>
</div>
