<%= formatting %>
<%= bootstrap_form_for(@wiki_page) do |f| %>
  <%= f.error_messages %>
  <%= f.fieldset "wiki_page" do %>
    <%= f.collection_select(:wiki_category_id, WikiCategory.where(deleted: false).order("title ASC"), :id, :title, prompt: true) %>
    <%= f.text_field :title %>
    <%= f.text_area :body %>
  <% end %>
  <%= f.submit "Submit", class: "btn" %>
<% end %>
