<% tabs.each do |sublink| %>
  <%
    active = false
    url = url_for_internal_tab(sublink)
    if url == request.fullpath
      active = true
      current_sublink = sublink
    end
    %>
  
  <%= link_to sublink.title, url, :class => active ? "a_sublink" : "ina_sublink"  if can_access_link(sublink) %>
<% end %>
